        <local:BugValueConverter x:Key="bugPathConverter" Ant="{StaticResource ant}" Beetle="{StaticResource beetle}" Spider="{StaticResource spider}" Bee="{StaticResource bee}" Grasshopper="{StaticResource grasshopper}"/>
        <local:ColorValueConverter x:Key="colorConverter" Black="#FF444444" White="White"/>
        <local:TimeSpanConverter x:Key="timeSpanConverter"/>
        <local:HexMarginConverter x:Key="hexMarginConverter"/>
        <local:DepthConverter x:Key="depthConverter"/>
        <local:PlayModeConverter x:Key="playModeConverter"/>
        <local:HeightToVisibilityConverter x:Key="heightToVisibilityConverter"/>
        <local:BoolToBrushConverter x:Key="boolToBrushConverter" TrueBrush="Yellow"/>
        <local:GameStatusConverter x:Key="gameStatusConverter"/>
        <local:BoolToVisibilityConverter x:Key="boolToVisibilityConverter"/>
        <local:TileColorConverter x:Key="tileColorConverter" Empty="#FFF7F7F7" Black="#FF444444"/>
        <DataTemplate x:Key="tileTemplate">
            <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                <Grid Width="40" Height="40">
                    <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                        <Grid>
                            <Polygon MouseMove="PolygonInReserve_MouseMove" Points="150,173.21 50,173.21 0,86.6 50,0 150,0 200,86.6 150,173.21" Stroke="#FFE2E2E2" StrokeThickness="3" Fill="{Binding Color, Converter={StaticResource colorConverter}}"/>
                            <Grid IsHitTestVisible="False">
                                <Grid.Background>
                                    <VisualBrush Visual="{Binding Bug, Converter={StaticResource bugPathConverter}}"/>
                                </Grid.Background>
                            </Grid>
                        </Grid>
                    </Viewbox>
                </Grid>
            </StackPanel>
        </DataTemplate>
        <Style TargetType="Border" x:Key="actionRegion">
            <Setter Property="BorderThickness" Value="3"></Setter>
        </Style>
        <Style TargetType="{x:Type ListBox}" x:Key="tileReserve">
            <Setter Property="ItemTemplate" Value="{StaticResource ResourceKey=tileTemplate}"/>
            <Setter Property="ScrollViewer.HorizontalScrollBarVisibility" Value="Disabled"/>
            <Setter Property="ItemsPanel">
                <Setter.Value>
                    <ItemsPanelTemplate>
                        <WrapPanel/>
                    </ItemsPanelTemplate>
                </Setter.Value>
            </Setter>
        </Style>
    </Window.Resources>
    <Window.DataContext>
        <Binding Mode="OneWay" Source="{StaticResource viewModel}"/>
    </Window.DataContext>
    <Grid>
        <Slider x:Name="zoomSlider" Margin="65,545,0,0"  Minimum="0.3" Maximum="3" Value="1" Height="18" VerticalAlignment="Top" HorizontalAlignment="Left" Width="166"/>
        <Border HorizontalAlignment="Left" Margin="10,10,0,0" Width="238" Height="143" VerticalAlignment="Top" Style="{DynamicResource actionRegion}" BorderBrush="{Binding CanDragReserveBlack, Converter={StaticResource boolToBrushConverter}, Mode=OneWay}" >
            <ListBox x:Name="reserveBlackListBox" ItemsSource="{Binding ReserveBlack}" Style="{DynamicResource tileReserve}" SelectedItem="{Binding SelectedReserveBlack}" IsEnabled="{Binding CanDragReserveBlack, Mode=OneWay}" ScrollViewer.VerticalScrollBarVisibility="Disabled"/>
        </Border>
        <Border HorizontalAlignment="Left" Height="141" Margin="10,158,0,0" VerticalAlignment="Top" Width="238" Style="{DynamicResource actionRegion}" BorderBrush="{Binding CanDragReserveWhite, Converter={StaticResource boolToBrushConverter}, Mode=OneWay}" >
            <ListBox x:Name="reserveWhiteListBox" ItemsSource="{Binding ReserveWhite}" Style="{DynamicResource tileReserve}" SelectedItem="{Binding SelectedReserveWhite}" IsEnabled="{Binding CanDragReserveWhite, Mode=OneWay}" ScrollViewer.VerticalScrollBarVisibility="Disabled"/>
        </Border>
        <Border Margin="253,10,10,10" Style="{DynamicResource actionRegion}" BorderBrush="{Binding CanDragTileOnBoard, Converter={StaticResource boolToBrushConverter}, Mode=OneWay}" >
            <ListBox x:Name="boardListBox" ItemsSource="{Binding Stacks}" SelectedItem="{Binding SelectedInPlay}" IsEnabled="{Binding CanDragTileOnBoard, Mode=OneWay}">
                <ListBox.ItemsPanel>
                    <ItemsPanelTemplate>
                        <Grid>
                            <Grid.LayoutTransform>
                                <TransformGroup>
                                    <ScaleTransform ScaleX="{Binding Value, ElementName=zoomSlider}" ScaleY="{Binding Value, ElementName=zoomSlider}"/>
                                    <SkewTransform/>
                                    <RotateTransform/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Grid.LayoutTransform>
                        </Grid>
                    </ItemsPanelTemplate>
                </ListBox.ItemsPanel>
                <ListBox.ItemContainerStyle>
                    <Style TargetType="{x:Type ListBoxItem}">
                        <Setter Property="HorizontalAlignment" Value="Center"/>
                        <Setter Property="VerticalAlignment" Value="Center"/>
                        <Setter Property="VerticalContentAlignment" Value="Center"/>
                        <Setter Property="HorizontalContentAlignment" Value="Center"/>
                        <Setter Property="Margin" Value="{Binding Hex, ConverterParameter=98, Converter={StaticResource hexMarginConverter}, Mode=OneWay}"/>
                        <Setter Property="Width" Value="100"/>
                        <Setter Property="Height" Value="83.60254038"/>
                        <Setter Property="Padding" Value="1"/>
                    </Style>
                </ListBox.ItemContainerStyle>
                <ListBox.ItemTemplate>
                    <DataTemplate>
                        <Grid d:DataContext="{d:DesignInstance Type={x:Type local:BoardHex}}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                            <Grid.Resources>
                                <Style TargetType="{x:Type Ellipse}" x:Key="dropTarget">
                                    <Setter Property="Visibility" Value="Hidden"/>
                                    <Setter Property="Fill" Value="Yellow"/>
                                    <Style.Triggers>
                                        <DataTrigger Binding="{Binding IsDropTarget}" Value="True">
                                            <Setter Property="Fill" Value="Green"/>
                                            <Setter Property="Visibility" Value="Visible"/>
                                        </DataTrigger>
                                    </Style.Triggers>
                                </Style>
                            </Grid.Resources>
                            <Viewbox HorizontalAlignment="Stretch" VerticalAlignment="Stretch">
                                <Grid>
                                    <Polygon AllowDrop="True" DragOver="PolygonInPlay_DragOver" Drop="PolygonInPlay_Drop" MouseMove="PolygonInPlay_MouseMove" Points="150,173.21 50,173.21 0,86.6 50,0 150,0 200,86.6 150,173.21" Stroke="#FFE2E2E2" StrokeThickness="3" Fill="{Binding Top, Converter={StaticResource tileColorConverter}}"/>
                                    <Grid IsHitTestVisible="False">
                                        <Grid.Background>
                                            <VisualBrush Visual="{Binding Top.Bug, Converter={StaticResource bugPathConverter}}"/>
                                        </Grid.Background>
                                        <Ellipse Style="{Binding Mode=OneWay, Source={StaticResource dropTarget}}" Width="30" Height="30" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                    </Grid>
                                </Grid>
                            </Viewbox>
                        </Grid>
                    </DataTemplate>
                </ListBox.ItemTemplate>
            </ListBox>
        </Border>
        <Button Content="Skip Turn" HorizontalAlignment="Left" Margin="16,590,0,0" VerticalAlignment="Top" Command="{Binding SkipTurn}" Width="76"/>
        <Button x:Name="startButton" Content="Start" HorizontalAlignment="Left" Margin="168,366,0,0" VerticalAlignment="Top" Width="63" Command="{Binding Start}" Height="22"/>
        <ComboBox x:Name="modeCombo" HorizontalAlignment="Left" Margin="16,366,0,0" VerticalAlignment="Top" Width="141" SelectedItem="{Binding GamePlayMode}" IsEnabled="{Binding IsGameNotInProgress, Mode=OneWay}">
            <ComboBox.Items>
                <local:PlayMode>HumanVersusHuman</local:PlayMode>
                <local:PlayMode>HumanGoesFirst</local:PlayMode>
                <local:PlayMode>ComputerGoesFirst</local:PlayMode>
            </ComboBox.Items>
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Converter={StaticResource playModeConverter}, Mode=OneWay}"></TextBlock>
                </DataTemplate>
            </ComboBox.ItemTemplate>
        </ComboBox>
        <ComboBox x:Name="thinkTimeCombo" HorizontalAlignment="Left" Margin="16,430,0,0" VerticalAlignment="Top" Width="120" SelectedItem="{Binding ThinkTime}">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Converter={StaticResource timeSpanConverter}, Mode=OneWay}"></TextBlock>
                </DataTemplate>
            </ComboBox.ItemTemplate>
            <ComboBox.Items>
                <System:TimeSpan>00:00:15</System:TimeSpan>
                <System:TimeSpan>00:00:30</System:TimeSpan>
                <System:TimeSpan>00:00:45</System:TimeSpan>
                <System:TimeSpan>00:01:00</System:TimeSpan>
                <System:TimeSpan>00:01:15</System:TimeSpan>
                <System:TimeSpan>00:01:30</System:TimeSpan>
                <System:TimeSpan>00:01:45</System:TimeSpan>
                <System:TimeSpan>00:02:00</System:TimeSpan>
            </ComboBox.Items>
        </ComboBox>
        <ComboBox x:Name="depthCombo" HorizontalAlignment="Left" Margin="16,497,0,0" VerticalAlignment="Top" Width="116" SelectedItem="{Binding ThinkDepth}">
            <ComboBox.ItemTemplate>
                <DataTemplate>
                    <TextBlock Text="{Binding Converter={StaticResource depthConverter}, Mode=OneWay}"></TextBlock>
                </DataTemplate>
            </ComboBox.ItemTemplate>
            <ComboBox.Items>
                <System:Int32>3</System:Int32>
                <System:Int32>4</System:Int32>
                <System:Int32>5</System:Int32>
                <System:Int32>6</System:Int32>
                <System:Int32>7</System:Int32>
                <System:Int32>8</System:Int32>
                <System:Int32>999</System:Int32>
            </ComboBox.Items>
        </ComboBox>
        <Label x:Name="modeLabel" Content="Mode:" HorizontalAlignment="Left" Margin="16,340,0,0" VerticalAlignment="Top" Target="{Binding ElementName=modeCombo, Mode=OneWay}"/>
        <Label x:Name="thinkTimeLabel" Content="Computer think time:" HorizontalAlignment="Left" Margin="16,404,0,0" VerticalAlignment="Top" Target="{Binding ElementName=thinkTimeCombo, Mode=OneWay}"/>
        <Button x:Name="abortButton" Content="Abort Game" HorizontalAlignment="Left" Margin="103,590,0,0" Width="102" Command="{Binding Abort}" VerticalAlignment="Top"/>
        <Border x:Name="statusBorder" HorizontalAlignment="Left" Margin="14,304,0,0" VerticalAlignment="Top" Background="Black" Width="230" >
            <TextBlock x:Name="gameStatusLabel" TextWrapping="Wrap" Text="{Binding GameStatus, Converter={StaticResource gameStatusConverter}, Mode=OneWay}" Height="16" Margin="3" Foreground="White" HorizontalAlignment="Center" FontWeight="Bold"/>
        </Border>
        <Label x:Name="depthLabel" Content="Max computer think-depth:" HorizontalAlignment="Left" Margin="16,471,0,0" VerticalAlignment="Top" Target="{Binding ElementName=depthCombo, Mode=OneWay}"/>
        <Label x:Name="zoomLabel" Content="Zoom:" HorizontalAlignment="Left" Margin="16,540,0,0" VerticalAlignment="Top" Target="{Binding ElementName=depthCombo, Mode=OneWay}"/>
        <ProgressBar x:Name="thinkProgress" HorizontalAlignment="Left" Height="9" Margin="14,331,0,0" VerticalAlignment="Top" Width="230" Value="{Binding ThinkProgress, Mode=OneWay}"/>
    </Grid>
</Window>
